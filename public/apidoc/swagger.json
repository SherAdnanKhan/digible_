                                    "city": {
                                        "type": "string",
                                        "example": "Bristol"
                                    },
                                    "dob": {
                                        "type": "date",
                                        "example": "1997-03-04"
                                    },
                                    "country_id": {
                                        "type": "integer",
                                        "example": 1
